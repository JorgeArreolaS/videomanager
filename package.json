{
  "name": "videomanager",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "backend",
    "web",
    "shared"
  ],
  "scripts": {
    "web": "powershell; cd ./web ; npm run start",
    "server": "powershell; cd ./backend ; nodemon",
    "start": "cd ./backend; npm run start",
    "build": "powershell; cd ./web ; npm run build; cd ../ ; npm run move; echo 'Done'",
    "move": "powershell; rm -r ./backend/public/* ; cp -r ./web/build/* ./backend/public/"
  },
